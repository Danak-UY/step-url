{
  "$configs": {
    "$dividers": [
      ".",
      ":",
      "-",
      "_",
      " ",
      "|",
      "/",
      "+"
    ],
    "$wildcard": "%s",
    "$comboWildcard": "cb"
  },
  "$combos": {
    "gh": {
      "_name": "Github",
      "_routes": [
        "gh",
        "gh-ry",
        "gh-ry-scripts"
      ],
      "ry": [
        "gh-ry",
        "gh-ry-scripts"
      ]
    }
  },
  "$routes": {
    "gh": {
      "_name": "GitHub",
      "_url": "https://github.com",
      "_search": "https://duckduckgo.com/?q=%s",
      "ry": {
        "_name": "Raycast",
        "_url": "https://github.com/raycast",
        "scripts": "https://github.com/raycast/script-commands"
      }
    },
    "dd": {
      "_name": "DataDog"
    }
  }
}